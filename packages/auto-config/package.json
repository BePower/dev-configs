{
  "name": "@bepower/auto-config",
  "version": "2.0.0",
  "description": "Common configuration and dependencies for npm deploy via auto package",
  "main": "./auto.config.js",
  "scripts": {
    "prebuild": "rimraf scripts/*.js ./*.js",
    "build": "tsc",
    "pretest": "npm run prebuild",
    "test": "jest"
  },
  "keywords": [
    "auto",
    "auto-config",
    "npm",
    "deploy"
  ],
  "author": "BePower",
  "license": "MIT",
  "dependencies": {
    "@auto-it/all-contributors": "^11.0.4",
    "@auto-it/conventional-commits": "^11.0.4",
    "@auto-it/core": "^11.0.4",
    "@auto-it/first-time-contributor": "^11.0.4",
    "@auto-it/magic-zero": "^11.0.4",
    "auto": "^11.0.4",
    "lerna": "^7.1.4"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.8.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "clover",
      "json",
      "json-summary",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "auto": {
    "plugins": [
      "magic-zero",
      "npm",
      [
        "conventional-commits",
        {
          "preset": "angular"
        }
      ],
      "all-contributors",
      "first-time-contributor"
    ]
  }
}
