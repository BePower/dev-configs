{
  "$schema": "https://raw.githubusercontent.com/lerna/lerna/main/packages/lerna/schemas/lerna-schema.json",
  "command": {
    "changed": {
      "includeMergedTags": true
    },
    "version": {
      "message": "chore: :bookmark: publish %s"
    }
  },
  "packages": ["packages/*"],
  "useNx": false,
  "version": "independent"
}
